{
  "pipeline": "eogrow.pipelines.sampling.FractionSamplingPipeline",
  "**global_config": "${config_path}/../global_config.json",
  "output_folder_key": "temp",
  "sampling_feature_name": "LULC_ID",
  "apply_to": {
    "reference": {
      "mask_timeless": [
        "LULC_ID",
        "POLYGON_ID"
      ]
    }
  },
  "fraction_of_samples": {
    "1": 1,
    "4": 0.4,
    "8": 0.8
  },
  "exclude_values": [0, 1],
  "mask_of_samples_name": "MASK_OF_SAMPLES",
  "seed": 21,
  "workers": 2
}

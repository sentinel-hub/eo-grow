    "**batch": "${config_path}/batch_area.json"
  },
  "eopatch": {
    "manager": "eogrow.core.eopatch.BatchTileManager"
  },
  "logging": {
    "manager": "eogrow.core.logging.LoggingManager",
    "save_logs": true,
    "show_logs": true,
    "capture_warnings": false
  },
  "storage": {
    "manager": "eogrow.core.storage.StorageManager",
    "project_folder": "${config_path}/../../test_project",
    "structure": {
      "grid_folder1": "grid-folder1",
      "grid_folder2": "grid-folder2",
      "grid_folder3": "grid-folder3"
    }
  }
}

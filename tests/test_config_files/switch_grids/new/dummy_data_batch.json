{
  "pipeline": "eogrow.pipelines.testing.DummyDataPipeline",
  "**global_config": "${config_path}/global_config.json",
  "eopatch": {
    "manager": "eogrow.core.eopatch.BatchTileManager"
  },
  "area": { // These parameters must match the content of test_project/input-data/batch_grid.gpkg grid file
    "manager": "eogrow.core.area.BatchAreaManager",
    "area_filename": "batch_area.geojson",
    "tiling_grid_id": 1,
    "resolution": 10,
    "tile_buffer_x": 3,
    "tile_buffer_y": 7
  },
  "output_folder_key": "input_folder",
  "seed": 42,
  "raster_features": [
    {
      "feature": ["data", "BANDS"],
      "shape": [2, 1014, 1006, 3],
      "dtype": "float32",
      "min_value": -1,
      "max_value": 1
    },
    {
      "feature": ["data", "IGNORED_BANDS"],
      "shape": [2, 1014, 1006, 3],
      "dtype": "float32",
      "min_value": -1,
      "max_value": 1
    },
    {
      "feature": ["mask_timeless", "REF"],
      "shape": [1014, 1006, 2],
      "dtype": "bool",
      "min_value": 0,
      "max_value": 1
    }
  ],
  "timestamp_feature": {
    "time_period": ["2022-02-01", "2022-03-01"],
    "timestamp_num": 2
  }
}

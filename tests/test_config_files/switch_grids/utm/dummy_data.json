{
  "pipeline": "eogrow.pipelines.testing.DummyDataPipeline",
  "**global_config": "${config_path}/global_config.json",
  "output_folder_key": "grid_folder1",
  "seed": 42,
  "raster_features": [
    {
      "feature": ["data", "BANDS"],
      "shape": [2, 112, 130, 3],
      "dtype": "float32",
      "min_value": -1,
      "max_value": 1
    },
    {
      "feature": ["mask", "dataMask"],
      "shape": [2, 112, 130, 1],
      "dtype": "uint8",
      "min_value": 4,
      "max_value": 6
    },
    {
      "feature": ["mask_timeless", "REF"],
      "shape": [112, 130, 2],
      "dtype": "bool",
      "min_value": 0,
      "max_value": 1
    },
    {
      "feature": ["label", "LABELS"],
      "shape": [2, 15],
      "dtype": "int16",
      "min_value": -2,
      "max_value": 5,
      "same_for_all": true
    },
    {
      "feature": ["scalar", "IGNORE"],
      "shape": [2, 5],
      "dtype": "float"
    }
  ],
  "timestamp_feature": {
    "time_period": ["2022-02-01", "2022-03-01"],
    "timestamp_num": 2,
    "same_for_all": true
  }
}

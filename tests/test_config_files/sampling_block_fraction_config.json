{
  "pipeline": "eogrow.pipelines.sampling.BlockSamplingPipeline",
  "**global_config": "${config_path}/global_config.json",
  "output_folder_key": "temp",
  "apply_to": {
    "reference": {
      "mask_timeless": [
        "LULC_ID"
      ]
    },
    "data": {
      "data": [
        "BANDS-S2-L1C"
      ],
      "mask": [
        "dataMask"
      ]
    }
  },
  "sample_size": [25, 30],
  "fraction_of_samples": 0.001,
  "mask_of_samples_name": "MASK_OF_SAMPLES",
  "seed": 100,
  "workers": 2
}
